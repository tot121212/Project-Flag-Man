[application]

config/name="Flag Man"
run/main_scene="res://scenes/menus/main_menu.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
config/icon="res://icon.svg"

[aseprite_importers]

aseprite_executable_path="C:/Users/evanm/Desktop/Art/3 - Resources/Aseprite/Aseprite v1.3-rc2/aseprite.exe"

[audio]

manager/sound/bus="Sound"
manager/sound/pool_1D_size=16
manager/sound/pool_2D_size=16
